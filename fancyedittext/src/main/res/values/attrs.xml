<?xml version="1.0" encoding="utf-8"?>
<resources>
    <declare-styleable name="FancyEditText">
        <attr name="fetTextColor" format="color|reference" />
        <attr name="fetTextStartSize" format="dimension|reference" />
        <attr name="fetTextScale" format="float" />
        <attr name="fetDuration" format="integer" />
        <attr name="fetAnimType" format="enum">
            <enum name="UP" value="0" />
            <enum name="DOWN" value="1" />
        </attr>
    </declare-styleable>
</resources>